                        cuNet()
                        #nullMac()
                        cuMac(10, 10, 1, 1, 1)
			#csmaca(0, 0, 10, 10, 1, 1, 1)
                        cc2420(26, 31, 1, 1)
                        #nullRadio()
}


configuration blinking1 { blink(1, 500)
			nullNet()
                        nullMac()
                        nullRadio()
}

configuration blinking2 { blink(2, 500)
			nullNet()
                        nullMac()
                        nullRadio()
}


event timerMonitor {
                        timerMilliE(5000, 12)
                        #timerMilliE(3000, 0xFFFF)
